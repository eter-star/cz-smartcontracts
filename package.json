{
	"name": "cz-smartcontracts",
	"version": "1.0.0",
	"description": "Cryptozombies smart contracts",
	"main": "truffle-config.js",
	"directories": {
		"test": "test"
	},
	"scripts": {
		"test": "rm -rf build && truffle test",
		"server": "ganache-cli",
		"build": "truffle compile",
		"coverage": "rm -rf build && truffle run coverage",
		"migrate": "truffle migrate --network ropsten",
		"docker:test": "docker-compose -p cz-test -f docker-compose.tests.yml up",
		"docker:migrate": "docker-compose -p cz-migrate -f docker-compose.migrate.yml up"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/tnguyen42/cz-smartcontracts.git"
	},
	"author": "",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/tnguyen42/cz-smartcontracts/issues"
	},
	"homepage": "https://github.com/tnguyen42/cz-smartcontracts#readme",
	"dependencies": {
		"@truffle/hdwallet-provider": "^1.2.1",
		"chai": "^4.2.0",
		"dotenv": "^8.2.0",
		"prettier": "^2.2.1",
		"web3-utils": "^1.3.3"
	},
	"devDependencies": {
		"@openzeppelin/test-helpers": "^0.5.10",
		"solidity-coverage": "^0.7.13"
	}
}
